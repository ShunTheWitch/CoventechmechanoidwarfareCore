<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<ThingDef ParentName="BaseBullet">
		<defName>Reavergats</defName>
		<label>Shockround</label>
    		<thingClass>Bullet</thingClass>
		
		<graphicData>
      			<texPath>Things/projectiles/RedEnergy_Shot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
		</graphicData>
		
		<projectile>
			<damageDef>MechElecShockbullet</damageDef>
			<damageAmountBase>13</damageAmountBase>
			<speed>100</speed>
		</projectile>
		<comps>
      <li Class="MYDE_AerocraftFramework.CompProperties_SpawnFleck_Projectile">
        <!--生成的特效-->
        <FleckDef>BlastEMP</FleckDef>
        <!--这个是播放特效的间隔-->
        <Fleck_MakeFleckTickMax>1</Fleck_MakeFleckTickMax>
        <!--这个是播放多少次特效-->
        <Fleck_MakeFleckNum>1~2</Fleck_MakeFleckNum>
        <!--这个是特效贴图旋转角度-->
        <Fleck_Rotation>-180~180</Fleck_Rotation>
        <!--这个是特效飞行方向-->
        <Fleck_Angle>-10~10</Fleck_Angle>
        <!--这个是特效大小-->
        <Fleck_Scale>0.2~0.5</Fleck_Scale>
        <!--这个是特效飞行速度-->
        <Fleck_Speed>5~7</Fleck_Speed>
      </li>
		</comps>
	</ThingDef>
</Defs>