<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>CVNMechComposite</defName>
    <label>Mech Composite materials</label>
    <description>Carefully layered steel plasteel composite plates made to cover the majority of a large mechanoid's hull, made to compress the cost to save time for when assembly of a large mechanoid is needed.</description>
    <graphicData>
      <texPath>Things/universal/Resources/PlasteelComposite/SuperMetal</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>75</stackLimit>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <MarketValue>560</MarketValue>
      <Mass>55</Mass>
      <Flammability>0.0</Flammability>
      <WorkToMake>100</WorkToMake>
    </statBases>
    <thingCategories>
      <li>ItemsMisc</li>
    </thingCategories>
    <drawGUIOverlay>true</drawGUIOverlay>
    <techLevel>Spacer</techLevel>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <recipeMaker>
      <researchPrerequisite>BasicMechtech</researchPrerequisite>
      <workSpeedStat>SubcoreEncodingSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <recipeUsers>
        <li>SubcoreEncoder</li>
      </recipeUsers>
      <mechanitorOnlyRecipe>true</mechanitorOnlyRecipe>
      <unfinishedThingDef>UnfinishedSubcore</unfinishedThingDef>
      <soundWorking>SubcoreEncoder_Working</soundWorking>
    </recipeMaker>
    <costList>
      <Steel>200</Steel>
      <Plasteel>20</Plasteel>
    </costList>
  </ThingDef>


